{
  "name": "devforum-search",
  "displayName": "Devforum Search",
  "description": "An extension that lets you search the Roblox devforum straight from your code editor.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "devforum-search.search-devforum",
      "title": "Search the devforum",
      "shortTitle": "Devforum Search"
    },
    {
      "command": "devforum-search.view-github",
      "title": "View devforum search on Github",
      "shortTitle": "View on Github!"
    }
  ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.91.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}
